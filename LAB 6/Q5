import pandas as pd

data = {
    'YEAR': [1986, 1986, 1985],
    'WHO Region': ['Western Pacific', 'Americas', 'Africa'],
    'Country': ['Vietnam', 'Uruguay', "CÃ´te d'Ivoire"],
    'Beverage Types': ['Vine', 'Other', 'Wine'],
    'Display Value': [0, 0.5, 1.62]
}

